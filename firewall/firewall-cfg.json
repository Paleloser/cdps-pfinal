{
  "image": "images:ubuntu/18.04",
  "name": "firewall",
  "dependencies": [
    "lynx",
    "net-tools",
    "tcpdump",
    "traceroute",
    "nmap",
    "fwbuilder"
  ],
  "interfaces": [
    {
      "name": "eth0",
      "network": "default"
    },
    {
      "name": "eth1",
      "network": "intra-lan0",
      "address": "10.1.0.1/16",
      "gateway4": "10.1.0.2"
    }
  ],
  "forwarding": true,
  "replication": 0
}